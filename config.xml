<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns = "http://www.w3.org/ns/widgets"
    xmlns:gap = "http://phonegap.com/ns/1.0"
	xmlns:android   = "http://schemas.android.com/apk/res/android"
    id        = "com.migente.android.cell"
    version   = "13.21.00"
	versionCode = "1448">

<name>Revista Mi Gente TV</name>

<description>
Revista Mi Gente TV
</description>

<author href="http://vistad.co" email="vista@vistad.co">
Royal Media
</author>

<access origin="*" subdomains="true" />

<plugin name="cordova-plugin-inappbrowser" spec="0.6.0" source="npm" />
<plugin name="cordova-plugin-splashscreen" spec="1.0.0" source="npm" />
<plugin name="cordova-plugin-whitelist" spec="1.3.0" source="npm" />

<allow-navigation href="*://*.youtube.com/*" />
<allow-navigation href="*://*.youtu.be/*" />

<preference name="phonegap-version" value="cli-6.5.0" />
<preference name="SplashScreen" value="splash.png" />
<preference name="SplashScreenDelay" value="5000" />
<preference name="orientation" value="portrait" />
<preference name="fullscreen" value="true" />
<preference name="permissions" value="none"/>


<feature name="http://api.phonegap.com/1.0/media"/>
<feature name="http://api.phonegap.com/1.0/network"/>
<feature name="http://api.phonegap.com/1.0/device" />
<feature name="http://api.phonegap.com/1.0/file" />

<icon src="icon.png" />
<icon src="iconos/android/icono36x36.png" platform="android" qualifier="ldpi" />
<icon src="iconos/android/icono48x48.png" platform="android" qualifier="mdpi" />
<icon src="iconos/android/icono72x72.png" platform="android" qualifier="hdpi" />
<icon src="iconos/android/icono144x144.png" platform="android" qualifier="xhdpi" />

<splash src="splash.png" />
<splash src="splash/android/splash200x320.png" platform="android" qualifier="ldpi" />
<splash src="splash/android/splash320x480.png" platform="android" qualifier="mdpi" />
<splash src="splash/android/splash480x800.png" platform="android" qualifier="hdpi" />
<splash src="splash/android/splash720x1280.png" platform="android" qualifier="xhdpi" />

</widget>